<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ccnu.library.data.UserinfoEntity" table="userinfo" schema="" catalog="librarydatabase">
        <id name="id">
            <column name="ID" sql-type="int" not-null="true"/>
        </id>
        <property name="username">
            <column name="Username" sql-type="varchar" length="20" not-null="true"/>
        </property>
        <property name="password">
            <column name="Password" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="rented">
            <column name="Rented" sql-type="varchar" length="30"/>
        </property>
        <set name="requestsById" inverse="true">
            <key>
                <column name="UserID"/>
            </key>
            <one-to-many not-found="ignore" class="com.ccnu.library.data.RequestEntity"/>
        </set>
    </class>
</hibernate-mapping>