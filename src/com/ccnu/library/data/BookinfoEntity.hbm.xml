<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ccnu.library.data.BookinfoEntity" table="bookinfo" schema="" catalog="librarydatabase">
        <id name="id">
            <column name="ID" sql-type="int" not-null="true"/>
        </id>
        <property name="name">
            <column name="Name" sql-type="varchar" length="20"/>
        </property>
        <property name="author">
            <column name="Author" sql-type="varchar" length="10"/>
        </property>
        <property name="path">
            <column name="Path" sql-type="varchar" length="20"/>
        </property>
    </class>
</hibernate-mapping>