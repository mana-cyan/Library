<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ccnu.library.data.RequestEntity" table="request" schema="" catalog="librarydatabase">
        <id name="id">
            <column name="ID" sql-type="int" not-null="true"/>
        </id>
        <property name="bookId">
            <column name="BookID" sql-type="int" not-null="true"/>
        </property>
        <property name="userId">
            <column name="UserID" sql-type="int" not-null="true"/>
        </property>
        <property name="fromDate">
            <column name="FromDate" sql-type="varchar" length="20" not-null="true"/>
        </property>
        <property name="toDate">
            <column name="ToDate" sql-type="varchar" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>